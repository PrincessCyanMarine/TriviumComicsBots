{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/node_modules": true,
        "**/dist": true
    },
    "json.schemas": [
        {
            "fileMatch": ["*.botmeta"],
            "url": "https://raw.githubusercontent.com/PrincessCyanMarine/TriviumComicsBots/master/schema/botmeta.json"
        }
    ],
    "files.associations": {
        "*.botmeta": "json"
    }
}
